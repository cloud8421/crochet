{
  "name": "crochet",
  "version": "0.0.0",
  "description": "A React/Flux app generate by RF, powered with Babel/Webpack",
  "main": "build/bundle.js",
  "dependencies": {
    "firebase": "^2.2.0",
    "flux": "^2.0.1",
    "immutable": "^3.6.2",
    "lodash": "*",
    "node-bourbon": "^1.2.3",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-immutable-render-mixin": "^0.8.0",
    "react-router": "^0.12.0",
    "react-simple-colorpicker": "^1.0.1"
  },
  "devDependencies": {
    "babel-jest": "*",
    "babel-loader": "*",
    "css-loader": "*",
    "file-loader": "*",
    "jest-cli": "*",
    "react-hot-loader": "^1.1.4",
    "react-tools": "^0.12.2",
    "sass-loader": "*",
    "style-loader": "*",
    "url-loader": "^0.5.5",
    "webpack": "^1.4.14",
    "webpack-dev-server": "^1.7.0"
  },
  "scripts": {
    "dev": "webpack-dev-server --colors --content-base ./build",
    "test": "jest",
    "build": "webpack --config webpack.config.js --progress --profile --colors"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
